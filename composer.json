{
  "name": "check24/apitk-manipulation-bundle",
  "version": "1.0.3",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "",
  "authors": [
    {
      "name": "Alexander Dormann",
      "email": "alexdo@alexdo.de"
    }
  ],
  "autoload": {
    "psr-4": { "Shopping\\ApiTKManipulationBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/",
      "/example/"
    ]
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/byWulf/apitk-common-bundle.git"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "nelmio/api-doc-bundle": "^3.2",
    "sensio/framework-extra-bundle": "^5.2",
    "check24/apitk-common-bundle": "^2.0",
    "symfony/config": ">= 4.3 <6.0",
    "symfony/dependency-injection": ">= 4.3 <6.0",
    "symfony/form": ">= 4.3 <6.0",
    "symfony/framework-bundle": ">= 4.3 <6.0",
    "symfony/orm-pack": "^1.0"
  },
  "require-dev": {
    "captainhook/captainhook": "^5.0",
    "captainhook/plugin-composer": "^5.1",
    "friendsofphp/php-cs-fixer": "^2.12",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11",
    "phpunit/phpunit": "^7.0",
    "symfony/phpunit-bridge": "^4.0"
  }
}
